	[ "$FREETZ_PACKAGE_BINARY_TOOLS_AR" == "y" ] && FILES="$FILES /usr/bin/ar"
	[ "$FREETZ_PACKAGE_BINARY_TOOLS_RANLIB" == "y" ] && FILES="$FILES /usr/bin/ranlib"
